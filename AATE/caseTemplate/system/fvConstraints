FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvConstraints;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#includeConstraint limitTemperature(min=200, max=3000)
#includeConstraint limitPressure(min=1e4, max=1e30, minFactor=1, maxFactor=2)

